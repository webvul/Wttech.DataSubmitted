@{
    ViewBag.Title = "假期高速公路交通流量统计表";
    Layout = null;
}

<link href="~/Content/CSS/index/sjbs-report.css" rel="stylesheet" />
<script src="~/Scripts/My97DatePicker/WdatePicker.js"></script>
<script src="~/Scripts/report/HDayRoadSta.js"></script>
<style>
    .ui-widget-content .ui-state-default {
        background: #F7F9FB;
    }
</style>
<input type="hidden" value="18" id="ReportType" />
<input type="hidden" value="" id="IsEdit" />
<input type="hidden" id="defStartTime" value="@ViewBag.StartTime" />
<div id="big" class="big">
    <div class="gridName">
        <span class="spread"></span>
        <span>北京市路政局：假期高速公路交通流量统计表</span>
    </div>
    <div class="search-area">
        <form action="" method="post" id="queryForm1">
            <table class="queryTable">
                <tr>
                    <th width="60">数据日期：</th>
                    <td width="150">
                        <input id="holidayEnd" type="hidden" value='@ViewBag.EndTime' />
                        <input id="holidayStart" type="hidden" value='@ViewBag.StartTime' />
                        <input name="StartTime" type="text" class="input Wdate" id="StartTime" value="" onclick="WdatePicker({ dateFmt: 'yyyy年M月d日',minDate:'@ViewBag.StartTime',maxDate:'@ViewBag.EndTime' })">
                        <input name="StartTime" type="text" class="input Wdate" id="StartTime1" value="" onclick="WdatePicker({ dateFmt: 'yyyy年M月d日', maxDate: '%y-%M-%d' })" style="display:none;">
                    </td>
                    <th width="100" style="text-align:right">假期名称：</th>
                    <td width="150">
                        <select id="HolidayId_List" class="select">
                            @foreach (var item in (IEnumerable<SelectListItem>)ViewBag.HolName)
                            {
                                if (item.Value == ViewBag.HolidayId)
                                {
                                    <option value="@item.Value" selected="selected">@item.Text</option>
                                }
                                else
                                {
                                    <option value="@item.Value">@item.Text</option>
                                }
                            }
                        </select>
                    </td>
                    <td><input name="Input" type="button" value="查&nbsp;&nbsp;&nbsp;询" class="queryBtn1" id="query"></td>
                </tr>
            </table>
        </form>
    </div>
    <div id="creatReport" class="creatReport">
        <div id="operate_btn" class="operate_btn">
            <div class="queryBtn" id="upd">
                <span class="query_icon updIcon"></span>
                <span class="query_txt">修改</span>
            </div>
            <div class="queryBtn" id="saveBtn" style="display:none;">
                <span class="query_icon saveIcon"></span>
                <span class="query_txt">保存</span>
            </div>
            <div class="queryBtn" id="cancleBtn" style="display:none;">
                <span class="query_icon cancleIcon"></span>
                <span class="query_txt">取消</span>
            </div>
            <div class="queryBtn" id="export">
                <span class="query_icon expIcon"></span>
                <span class="query_txt">导出</span>
            </div>
            <div class="queryBtn" id="check">
                <span class="query_icon checkIcon"></span>
                <span class="query_txt">校正</span>
            </div>
        </div>
        <div class="report_title" style="text-align:center;color:#555;font-size:14px;">
            <span id="report18_name"></span>假期高速公路交通流量统计表
            <span id="noDataList" style="float:right;color:red;cursor:pointer;margin-right:15px; display:none;">无数据收费站列表</span>
        </div>
        <div class="report_title">
            <div style="margin-left:20px;margin-right:0;">
                填报单位：<span id="orgName">华北高速</span>
                <div style="float:right;margin-right:15px;">数据日期：<span id="date"></span></div>
            </div>
        </div>
        <div class="tableDiv">
            <table id="report18"></table>
        </div>
    </div>
</div>

