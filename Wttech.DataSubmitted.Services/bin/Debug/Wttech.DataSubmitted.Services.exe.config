<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!--sectionGroup自定义节点信息 注：需添加在配置文件最前面-->
    <sectionGroup name="IpGroup">
      <section name="ServerIp" type="System.Configuration.NameValueSectionHandler" />
    </sectionGroup>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection,Microsoft.Practices.Unity.Configuration" />
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <log4net>
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <!--定义文件存放位置-->
      <param name="File" value="Logs/Log.txt" />
      <param name="AppendToFile" value="true" />
      <param name="MaxSizeRollBackups" value="100" />
      <param name="MaximumFileSize" value="1MB" />
      <param name="RollingStyle" value="Size" />
      <param name="StaticLogFileName" value="true" />
      <param name="MaxSizeRollBackups" value="100"/>
      <layout type="log4net.Layout.PatternLayout">
        <!--每条日志末尾的文字说明-->
        <!--输出格式-->
        <!--样例：2008-03-26 13:42:32,111 [10] INFO  Log4NetDemo.MainClass [(null)] - info-->
        <param name="ConversionPattern" value="%newline %date：%-5level %message"/>
        <!--"%date-线程:[%thread] 级别：%-5level - 描述：%message%newline" -->
      </layout>
    </appender>
    <logger name="DataSubmittedLogger">
      <level value="All" />
      <appender-ref ref="RollingLogFileAppender" />
    </logger>
  </log4net>
  <!--远程服务器配置-->
  <IpGroup>
    <ServerIp>
      <add key="IP" value="172.16.2.63" />
      <add key="UserName" value="sa" />
      <add key="PassWord" value="tayh@2013" />
    </ServerIp>
  </IpGroup>
  <!--本地数据库连接串-->
  <connectionStrings>
    <add name="DataSubmittedEntities" connectionString="metadata=res://*/DataSubmittedModel.csdl|res://*/DataSubmittedModel.ssdl|res://*/DataSubmittedModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=.;initial catalog=DataSubmitted;persist security info=True;user id=sa;password=123456;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
  </connectionStrings>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="mssqllocaldb" />
      </parameters>
    </defaultConnectionFactory>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
  <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
    <namespace name="Wttech.DataSubmitted.IBLL" />
    <namespace name="Wttech.DataSubmitted.BLL" />
    <assembly name="Wttech.DataSubmitted.IBLL" />
    <assembly name="Wttech.DataSubmitted.BLL" />
    <containers>
      <container name="serviceContainer">
        <!--<register type="命名空间.接口类型1,命名空间" mapTo="命名空间.实现类型1,命名空间" />-->
        <!--<register type="Wttech.DataSubmitted.IBLL.IUserManage,Wttech.DataSubmitted.IBLL" mapTo="Wttech.DataSubmitted.BLL.UserManage,Wttech.DataSubmitted.BLL" />-->
        <register type="IReport.IStagingReport" mapTo="Report.StagingReport" >
          <lifetime type="HierarchicalLifetimeManager" />
        </register>
      </container>
    </containers>
  </unity>
</configuration>